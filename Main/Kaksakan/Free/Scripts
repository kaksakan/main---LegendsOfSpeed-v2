local Tab = Window:NewTab("Islands")
local Section = Tab:NewSection("Islands")

local plrs = {}
for _, v in pairs(game.Players:GetPlayers()) do
    table.insert(plrs, v.Name)
end

Section:NewDropdown("Chose A Player", "Chose A Player To Teleport That Player You Chose", plrs, function(currentOption)
    local selectedPlayer = game.Players:FindFirstChild(currentOption)
    if selectedPlayer then 
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame
    end
end)

Section:NewButton("City", "Click to teleport to the City", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-536.4736938476562, 4.126866340637207, 213.64923095703125)
end)

Section:NewButton("Snow Island", "Click to teleport to the Snow Island", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-821.5850219726562, 4.126863956451416, 2198.53466796875)
end)

Section:NewButton("Magma City", "Click to teleport to the Magma City", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1637.136962890625, 4.226874351501465, 4332.80078125)
end)

Section:NewButton("Legends Highway", "Click to teleport to the Legends Highway", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3672.014892578125, 71.70681762695312, 5593.314453125)
end)

