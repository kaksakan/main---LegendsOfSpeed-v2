local Tab = Window:NewTab("Eggs")
local Section = Tab:NewSection("City")

Section:NewButton("Blue Crystal", "Opens a Blue Crystal", function()

    local args = {
        [1] = "openCrystal",
        [2] = "Blue Crystal"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "Check Your İnventory!";
        Text  = "You bought a Egg";
        Icon  = "rbxthumb://type=Asset&id=17328930447&w=150&h=150"})
    Duration = 10;

end)

Section:NewButton("Yellow Crystal", "Opens a Yellow Crystal", function()
    local args = {
